        "telegram_bot_token": "REPLACE_ME",
        "telegram_chat_id": "REPLACE_ME"
    },
    "health_check_interval_minutes": 30,
    "nlp_settings": {
        "max_words": 300,
        "confidence_threshold": 0.4,
        "enable_caching": true,
        "cache_size": 1000
    },
    "scraper_interval_minutes": 10,
    "scrapers": {
        "craigslist": {
            "cities": ["sfbay", "newyork", "seattle", "losangeles", "boston", "austin"],
